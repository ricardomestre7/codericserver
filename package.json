{
  "name": "codserver-super-app",
  "version": "2.0.0",
  "description": "Super App - Plataforma Completa de Desenvolvimento com IA",
  "author": "Ricardo R. Pereira - Cyntreon",
  "license": "MIT",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "build:production": "cd client && npm run build && npm run optimize",
    "start": "node server/index.js",
    "install-all": "npm install && cd client && npm install",
    "deploy": "npm run build:production && npm run deploy:server",
    "deploy:server": "node scripts/deploy.js",
    "docker:build": "docker build -t codserver-super-app .",
    "docker:run": "docker run -p 3001:3001 codserver-super-app",
    "test": "npm run test:server && npm run test:client",
    "test:server": "jest server/",
    "test:client": "cd client && npm test"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.30.0",
    "@monaco-editor/react": "^4.7.0",
    "axios": "^1.6.2",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "fs-extra": "^11.1.1",
    "openai": "^4.20.0",
    "path": "^0.12.7",
    "simple-git": "^3.19.1",
    "socket.io": "^4.7.4",
    "multer": "^1.4.5-lts.1",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.3",
    "redis": "^4.6.10",
    "nodemailer": "^6.9.7",
    "sharp": "^0.32.6",
    "pdf-lib": "^1.17.1",
    "qrcode": "^1.5.3",
    "chart.js": "^4.4.0",
    "moment": "^2.29.4",
    "lodash": "^4.17.21",
    "uuid": "^9.0.1",
    "compression": "^1.7.4",
    "helmet": "^7.1.0",
    "rate-limiter-flexible": "^2.4.1",
    "express-validator": "^7.0.1",
    "swagger-ui-express": "^5.0.0",
    "swagger-jsdoc": "^6.2.8"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "@types/jest": "^29.5.8"
  },
  "keywords": [
